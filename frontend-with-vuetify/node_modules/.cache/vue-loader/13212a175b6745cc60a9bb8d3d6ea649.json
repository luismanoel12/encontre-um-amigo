ss=\"perfil-header\">\r\n        <div class=\"gravatar-img\">\r\n          <Gravatar :email=\"user.email\" alt=\"User\" />\r\n          <v-badge\r\n            v-if=\"user.admin\"\r\n            color=\"blue\"\r\n            icon=\"mdi-check-bold\"\r\n            bottom\r\n            overlap\r\n          ></v-badge>\r\n        </div>\r\n        <h1>{{ userData.name }}</h1>\r\n      </div>\r\n      <div class=\"perfil-content\">\r\n        <v-row>\r\n          <v-col cols=\"12\" md=\"3\" sm=\"12\">\r\n            <v-text-field\r\n              label=\"Nome\"\r\n              v-model=\"userData.name\"\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n          <v-col cols=\"12\" md=\"3\" sm=\"12\">\r\n            <v-text-field\r\n              label=\"E-mail\"\r\n              v-model=\"userData.email\"\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n          <v-col cols=\"12\" md=\"3\" sm=\"12\">\r\n            <v-text-field\r\n              v-if=\"userData.ong\"\r\n              v-model=\"userData.cnpj\"\r\n              label=\"CNPJ\"\r\n              v-mask=\"'##.###.###/####-##'\"\r\n              outlined\r\n            ></v-text-field>\r\n            <v-text-field\r\n              v-else\r\n              label=\"CPF\"\r\n              v-model=\"userData.cpf\"\r\n              v-mask=\"'###.###.###-##'\"\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n          <v-col cols=\"12\" md=\"3\" sm=\"12\">\r\n            <v-text-field\r\n              label=\"Telefone\"\r\n              v-model=\"userData.telefone\"\r\n              v-mask=\"'(##) #####-####'\"\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n        </v-row>\r\n\r\n        <v-row>\r\n          <v-col cols=\"12\" md=\"6\" sm=\"12\">\r\n            <v-text-field\r\n              label=\"Senha\"\r\n              v-model=\"userData.password\"\r\n              type=\"password\"\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n          <v-col cols=\"12\" md=\"6\" sm=\"12\">\r\n            <v-text-field\r\n              label=\"Confime sua Senha\"\r\n              v-model=\"userData.confirmPassword\"\r\n              type=\"password\"\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n        </v-row>\r\n\r\n        <v-divider class=\"divider-endereco\"></v-divider>\r\n\r\n        <v-row>\r\n          <v-col cols=\"12\" md=\"3\" sm=\"12\">\r\n            <v-text-field\r\n              label=\"Endereço\"\r\n              v-model=\"userData.endereco\"\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n          <v-col cols=\"12\" md=\"3\" sm=\"12\">\r\n            <v-text-field\r\n              label=\"Bairro\"\r\n              v-model=\"userData.bairro\"\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n          <v-col cols=\"12\" md=\"3\" sm=\"12\">\r\n            <v-text-field\r\n              label=\"Número\"\r\n              v-model=\"userData.numero\"\r\n              type=\"number\"\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n          <v-col cols=\"12\" md=\"3\" sm=\"12\">\r\n            <v-text-field\r\n              label=\"Complemento\"\r\n              v-model=\"userData.complemento\"\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n        </v-row>\r\n\r\n        <v-row>\r\n          <v-col cols=\"12\" md=\"4\" sm=\"12\">\r\n            <v-text-field\r\n              label=\"Estado\"\r\n              v-model=\"userData.estado\"\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n          <v-col cols=\"12\" md=\"4\" sm=\"12\">\r\n            <v-text-field\r\n              label=\"Cidade\"\r\n              v-model=\"userData.cidade\"\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n          <v-col cols=\"12\" md=\"4\" sm=\"12\">\r\n            <v-text-field\r\n              label=\"CEP\"\r\n              v-model=\"userData.cep\"\r\n              v-mask=\"'#####-###'\"\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n        </v-row>\r\n        <v-btn tile color=\"success\" @click=\"update\">\r\n          <v-icon left> mdi-account-edit </v-icon>\r\n          Atualizar\r\n        </v-btn>\r\n      </div>\r\n    </div>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport { userKey } from \"@/global\";\r\nimport { mapState } from \"vuex\";\r\nimport Gravatar from \"vue-gravatar\";\r\nimport { showError } from \"@/global\";\r\nimport api from \"../../config/api\";\r\n\r\nexport default {\r\n  name: \"Perfil\",\r\n  components: { Gravatar },\r\n  computed: mapState([\"user\"]),\r\n\r\n  data: function () {\r\n    return {\r\n      userData: {},\r\n      loading: true,\r\n    };\r\n  },\r\n\r\n  mounted() {\r\n    this.loadUsers()\r\n    this.$root.$once('user-updated', () => {\r\n            this.loadUsers()\r\n        })\r\n  },\r\n  methods: {\r\n    async loadUsers() {\r\n      const url = `/users/${this.user.id}`;\r\n      await api.get(url).then((res) => {\r\n        this.userData = res.data\r\n        this.loading = false\r\n      }).catch(erro => {\r\n      });\r\n    },\r\n    update() {\r\n      api\r\n        .put(`/users/${this.user.id}`, this.userData)\r\n        .then(() => {\r\n          this.$toasted.global.defaultSuccess();\r\n        })\r\n        .catch(showError);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n<style>\r\n.perfil-main {\r\n  background-color: #fff;\r\n  padding: 30px;\r\n  border-radius: 5px;\r\n}\r\n\r\n.perfil-header {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-content: center;\r\n  align-items: center;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.gravatar-img img {\r\n  border-radius: 50%;\r\n}\r\n\r\n.divider-endereco {\r\n  margin-bottom: 30px;\r\n}\r\n</style>"]}]}