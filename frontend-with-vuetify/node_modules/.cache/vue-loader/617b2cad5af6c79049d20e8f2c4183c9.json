ss=\"ma-2 avatar-img\" size=\"100\" tile>\r\n                  <Gravatar :email=\"ong.email\" :alt=\"ong.name\" />\r\n                </v-avatar>\r\n                <v-divider class=\"divider-avatar\" vertical></v-divider>\r\n                <div>\r\n                  <v-card-title class=\"headline text-center\">\r\n                    {{ ong.name }}\r\n                  </v-card-title>\r\n\r\n                  <div class=\"card-content\">\r\n                    <span\r\n                      >Estado: <strong>{{ ong.estado }}</strong>\r\n                    </span>\r\n                  </div>\r\n\r\n                  <v-card-actions> </v-card-actions>\r\n                </div>\r\n              </div>\r\n            </v-card>\r\n          </router-link>\r\n        </v-col>\r\n      </v-row>\r\n    </div>\r\n\r\n    <div class=\"text-center pagination\">\r\n      <v-btn depressed color=\"#00bfad\" dark v-if=\"loadMore\" @click=\"getOngs\">\r\n        Carregar Mais\r\n      </v-btn>\r\n    </div>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport api from \"../../config/api\";\r\nimport Gravatar from \"vue-gravatar\";\r\n\r\nexport default {\r\n  name: \"Ongs\",\r\n  components: { Gravatar },\r\n  data: function () {\r\n    return {\r\n      ongs: [],\r\n      page: 1,\r\n      loadMore: true,\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    getOngs() {\r\n      api(`/ongs?page=${this.page}`).then((res) => {\r\n        this.ongs = this.ongs.concat(res.data.data);\r\n        this.page++;\r\n\r\n        if (res.data.data.length === 0) this.loadMore = false;\r\n      });\r\n    },\r\n  },\r\n  watch: {\r\n    $route(to) {\r\n      this.ongs = [];\r\n      this.page = 1;\r\n      this.loadMore = true;\r\n\r\n      this.getOngs();\r\n    },\r\n  },\r\n  mounted() {\r\n    this.getOngs();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.ongs-page {\r\n  background-color: #fff;\r\n  border-radius: 10px;\r\n  margin-top: 20px;\r\n  padding: 20px;\r\n}\r\n\r\n.ong-img {\r\n  display: flex;\r\n  justify-content: center;\r\n  justify-items: center;\r\n  padding-top: 20px;\r\n}\r\n\r\n.ong-img > img {\r\n  max-height: 64px;\r\n  border-radius: 50%;\r\n}\r\n\r\n.v-card > *:last-child:not(.v-btn):not(.v-chip) {\r\n  padding: 0px !important;\r\n}\r\n\r\n.divider-avatar {\r\n  background-color: #fff;\r\n}\r\n\r\n.card-content {\r\n  padding: 10px;\r\n}\r\n\r\n.ong-card{\r\n  transition: cubic-bezier(0.175, 0.885, 0.32, 1.275) 0.5s;\r\n}\r\n\r\n.ong-card:hover{\r\n  transform: scale(1.03);\r\n  background-color: #52b69a!important;\r\n}\r\n</style>\r\n"]}]}