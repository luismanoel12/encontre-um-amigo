,EAAA;AAAA,OARA,EASA;AAAA,QAAA,IAAA,EAAA,OAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OATA;AAJA,KAAA;AAgBA,GAnBA;AAoBA,EAAA,OAAA,EAAA;AACA,IAAA,aADA,2BACA;AAAA;;AACA,UAAA,GAAA,cAAA;AACA,MAAA,GAAA,CAAA,GAAA,CAAA,GAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA;AACA,OAFA;AAGA,KANA;AAOA,IAAA,KAPA,mBAOA;AACA,WAAA,IAAA,GAAA,MAAA;AACA,WAAA,QAAA,GAAA,EAAA;AACA,WAAA,aAAA;AACA,KAXA;AAYA,IAAA,IAZA,kBAYA;AAAA;;AACA,UAAA,MAAA,GAAA,KAAA,QAAA,CAAA,EAAA,GAAA,KAAA,GAAA,MAAA;AACA,UAAA,EAAA,GAAA,KAAA,QAAA,CAAA,EAAA,cAAA,KAAA,QAAA,CAAA,EAAA,IAAA,EAAA;AACA,MAAA,GAAA,CAAA,MAAA,CAAA,oBAAA,EAAA,GAAA,KAAA,QAAA,EACA,IADA,CACA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,MAAA,CAAA,cAAA;;AACA,QAAA,MAAA,CAAA,KAAA;AACA,OAJA,EAKA,KALA,CAKA,SALA;AAMA,KArBA;AAsBA,IAAA,MAtBA,oBAsBA;AAAA;;AACA,UAAA,EAAA,GAAA,KAAA,QAAA,CAAA,EAAA;AACA,MAAA,GAAA,CACA,MADA,qBACA,EADA,GAEA,IAFA,CAEA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,MAAA,CAAA,cAAA;;AACA,QAAA,MAAA,CAAA,KAAA;AACA,OALA,EAMA,KANA,CAMA,SANA;AAOA,KA/BA;AAgCA,IAAA,YAhCA,wBAgCA,QAhCA,EAgCA;AAAA;;AAAA,UAAA,IAAA,uEAAA,MAAA;AACA,WAAA,IAAA,GAAA,IAAA;AACA,MAAA,GAAA,CACA,GADA,qBACA,QAAA,CAAA,EADA,GAEA,IAFA,CAEA,UAAA,GAAA;AAAA,eAAA,MAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA;AAAA,OAFA;AAGA;AArCA,GApBA;AA2DA,EAAA,KAAA,EAAA;AACA,IAAA,IADA,kBACA;AACA,WAAA,aAAA;AACA;AAHA,GA3DA;AAgEA,EAAA,OAhEA,qBAgEA;AACA,SAAA,aAAA;AACA;AAlEA,CAAA","sourcesContent":["<template>\r\n  <div class=\"carousel-page\">\r\n    <v-container>\r\n      <v-form class=\"form elevation-10\">\r\n        <h2 class=\"atencao\">ATENÇÃO: O tamanho da imagem deve ser 1440 x 500</h2>\r\n        <v-row>\r\n          <v-col cols=\"12\" sm=\"6\">\r\n            <v-text-field\r\n              label=\"Responsavel\"\r\n              v-model=\"carousel.author\"\r\n              :readonly=\"mode === 'remove'\"\r\n              required\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n          <v-col cols=\"12\" sm=\"6\">\r\n            <v-text-field\r\n              label=\"Link\"\r\n              v-model=\"carousel.link\"\r\n              :readonly=\"mode === 'remove'\"\r\n              required\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n        </v-row>\r\n\r\n        <div class=\"buttons\">\r\n          <v-btn depressed v-if=\"mode === 'save'\" @click=\"save\"  color=\"success\">\r\n            Salvar\r\n            <v-icon dark right> mdi-content-save </v-icon>\r\n          </v-btn>\r\n          <v-divider vertical></v-divider>\r\n          <v-btn\r\n            depressed\r\n            v-if=\"mode === 'remove'\"\r\n            @click=\"remove\"\r\n            color=\"error\"\r\n          >\r\n            Excluir\r\n             <v-icon dark right> mdi-delete </v-icon>\r\n          </v-btn>\r\n          <v-divider vertical></v-divider>\r\n          <v-btn depressed @click=\"reset\" class=\"btn-cancel\" color=\"primary\"> \r\n            Cancelar \r\n            <v-icon dark right> mdi-close-thick </v-icon>\r\n            </v-btn>\r\n        </div>\r\n      </v-form>\r\n\r\n      <v-data-table\r\n        :items=\"carousels\"\r\n        :headers=\"headers\"\r\n        class=\"elevation-10\"\r\n        \r\n      >\r\n        <template v-slot:[`item.actions`]=\"{ item }\">\r\n           <v-btn\r\n              class=\"bt-actions\"\r\n              color=\"primary\"\r\n              fab\r\n              elevation=\"0\"\r\n              small\r\n              @click=\"loadCarousel(item)\"\r\n              dark\r\n            >\r\n              <v-icon>mdi-pencil</v-icon>\r\n            </v-btn>\r\n\r\n           <v-btn\r\n              class=\"bt-actions\"\r\n              color=\"error\"\r\n              fab\r\n              elevation=\"0\"\r\n              small\r\n              @click=\"loadCarousel(item, 'remove')\"\r\n              dark\r\n            >\r\n              <v-icon>mdi-delete</v-icon>\r\n            </v-btn>\r\n          \r\n        </template>\r\n      </v-data-table>\r\n    </v-container>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { showError } from \"@/global\";\r\nimport api from \"../../config/api\";\r\n\r\nexport default {\r\n  name: \"Carousel\",\r\n  data: function () {\r\n    return {\r\n      mode: \"save\",\r\n      carousel: {},\r\n      carousels: [],\r\n      headers: [\r\n        {\r\n          align: \"start\",\r\n          sortable: false,\r\n          value: \"name\",\r\n        },\r\n        { text: \"Código\", value: \"id\" },\r\n        { text: \"Responsavel\", value: \"author\" },\r\n        { text: \"Link\", value: \"link\" },\r\n        { text: \"Ações\", value: \"actions\" },\r\n      ],\r\n    };\r\n  },\r\n  methods: {\r\n    loadCarousels() {\r\n      const url = `/carousel`;\r\n      api.get(url).then((res) => {\r\n        this.carousels = res.data;\r\n      });\r\n    },\r\n    reset() {\r\n      this.mode = \"save\";\r\n      this.carousel = {};\r\n      this.loadCarousels();\r\n    },\r\n    save() {\r\n      const method = this.carousel.id ? \"put\" : \"post\";\r\n      const id = this.carousel.id ? `/${this.carousel.id}` : \"\";\r\n      api[method](`/carousel${id}`, this.carousel)\r\n        .then(() => {\r\n          this.$toasted.global.defaultSuccess();\r\n          this.reset();\r\n        })\r\n        .catch(showError);\r\n    },\r\n    remove() {\r\n      const id = this.carousel.id;\r\n      api\r\n        .delete(`/carousel/${id}`)\r\n        .then(() => {\r\n          this.$toasted.global.defaultSuccess();\r\n          this.reset();\r\n        })\r\n        .catch(showError);\r\n    },\r\n    loadCarousel(carousel, mode = \"save\") {\r\n      this.mode = mode;\r\n      api\r\n        .get(`/carousel/${carousel.id}`)\r\n        .then((res) => (this.carousel = res.data));\r\n    },\r\n  },\r\n  watch: {\r\n    page() {\r\n      this.loadCarousels();\r\n    },\r\n  },\r\n  mounted() {\r\n    this.loadCarousels();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.form {\r\n  margin-top: 50px;\r\n  background-color: #fff;\r\n  padding: 30px;\r\n  border-radius: 5px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.bt-actions{\r\n    margin-left: 5px;\r\n}\r\n\r\n.bt-actions:hover{\r\n    transform: scale(1.1);\r\n}\r\n\r\n.btn-cancel{\r\n    margin-left: 10px;\r\n}\r\n\r\n.atencao{\r\n  text-align: center;\r\n  color: #FF1D23;\r\n  margin-bottom: 20px;\r\n}\r\n</style>"],"sourceRoot":"src/views/carousel"}]}