t-field\r\n              label=\"Valor Esperado\"\r\n              v-model=\"meta.valorEsperado\"\r\n              :readonly=\"mode === 'remove'\"\r\n              required\r\n              type=\"number\"\r\n              prefix=\"R$\"\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n          <v-col cols=\"12\" sm=\"3\">\r\n            <v-text-field\r\n              label=\"Valor Atual\"\r\n              v-model=\"meta.valorAtual\"\r\n              type=\"number\"\r\n              prefix=\"R$\"\r\n              :readonly=\"mode === 'remove'\"\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n          <v-col cols=\"12\" sm=\"3\">\r\n            <v-text-field\r\n              label=\"URL da imagem\"\r\n              v-model=\"meta.imageUrl\"\r\n              :readonly=\"mode === 'remove'\"\r\n              required\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n        </v-row>\r\n\r\n        <v-row>\r\n          <v-col cols=\"12\" sm=\"12\">\r\n            <VueEditor\r\n              v-model=\"meta.descricao\"\r\n              placeholder=\"Informe o que vai acontecer quando as metas forem atingidas.\"\r\n              :readonly=\"mode === 'remove'\"\r\n            />\r\n          </v-col>\r\n        </v-row>\r\n\r\n        <div class=\"buttons-metas\">\r\n          <v-btn depressed v-if=\"mode === 'save'\" @click=\"save\" color=\"success\">\r\n            Salvar\r\n          </v-btn>\r\n          <v-divider vertical></v-divider>\r\n          <v-btn\r\n            depressed\r\n            v-if=\"mode === 'remove'\"\r\n            @click=\"remove\"\r\n            color=\"error\"\r\n          >\r\n            Excluir\r\n          </v-btn>\r\n          <v-divider vertical></v-divider>\r\n          <v-btn depressed @click=\"reset\" class=\"btn-cancel\" color=\"primary\">\r\n            Cancelar\r\n          </v-btn>\r\n        </div>\r\n      </v-form>\r\n\r\n      <v-data-table :items=\"metas\" :headers=\"headers\" class=\"elevation-10\">\r\n        <template v-slot:[`item.actions`]=\"{ item }\">\r\n          <v-btn\r\n            class=\"bt-actions\"\r\n            color=\"primary\"\r\n            fab\r\n            elevation=\"0\"\r\n            small\r\n            @click=\"loadMeta(item)\"\r\n            dark\r\n          >\r\n            <v-icon>mdi-pencil</v-icon>\r\n          </v-btn>\r\n\r\n          <v-btn\r\n            class=\"bt-actions\"\r\n            color=\"error\"\r\n            fab\r\n            elevation=\"0\"\r\n            small\r\n            @click=\"loadMeta(item, 'remove')\"\r\n            dark\r\n          >\r\n            <v-icon>mdi-delete</v-icon>\r\n          </v-btn>\r\n        </template>\r\n      </v-data-table>\r\n    </v-container>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { showError } from \"@/global\";\r\nimport api from \"../../config/api\";\r\nimport { VueEditor } from \"vue2-editor\";\r\n\r\nexport default {\r\n  name: \"Metas\",\r\n  components: { VueEditor },\r\n  data: function () {\r\n    return {\r\n      mode: \"save\",\r\n      meta: {},\r\n      metas: [],\r\n      headers: [\r\n        {\r\n          align: \"start\",\r\n          sortable: false,\r\n          value: \"name\",\r\n        },\r\n        { text: \"Código\", value: \"id\" },\r\n        { text: \"Título\", value: \"titulo\" },\r\n        { text: \"Valor Esperado\", value: \"valorEsperado\" },\r\n        { text: \"Valor Atual\", value: \"valorAtual\" },\r\n        { text: \"Ações\", value: \"actions\" },\r\n      ],\r\n    };\r\n  },\r\n  methods: {\r\n    loadMetas() {\r\n      const url = `/metasUsuario`;\r\n      api.get(url).then((res) => {\r\n        this.metas = res.data;\r\n      });\r\n    },\r\n    reset() {\r\n      this.mode = \"save\";\r\n      this.meta = {};\r\n      this.loadMetas();\r\n    },\r\n    save() {\r\n      const method = this.meta.id ? \"put\" : \"post\";\r\n      const id = this.meta.id ? `/${this.meta.id}` : \"\";\r\n      api[method](`/metas${id}`, this.meta)\r\n        .then(() => {\r\n          this.$toasted.global.defaultSuccess();\r\n          this.reset();\r\n        })\r\n        .catch(showError);\r\n    },\r\n    remove() {\r\n      const id = this.meta.id;\r\n      api\r\n        .delete(`/metas/${id}`)\r\n        .then(() => {\r\n          this.$toasted.global.defaultSuccess();\r\n          this.reset();\r\n        })\r\n        .catch(showError);\r\n    },\r\n    loadMeta(meta, mode = \"save\") {\r\n      this.mode = mode;\r\n      api.get(`/metas/${meta.id}`).then((res) => (this.meta = res.data));\r\n    },\r\n  },\r\n  watch: {\r\n    page() {\r\n      this.loadMetas();\r\n    },\r\n  },\r\n  mounted() {\r\n    this.loadMetas();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.form {\r\n  margin-top: 50px;\r\n  background-color: #fff;\r\n  padding: 30px;\r\n  border-radius: 5px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.bt-actions {\r\n  margin-left: 5px;\r\n}\r\n\r\n.bt-actions:hover {\r\n  transform: scale(1.1);\r\n}\r\n\r\n.btn-cancel {\r\n  margin-left: 10px;\r\n}\r\n\r\n.buttons-metas {\r\n  margin-top: 20px;\r\n}\r\n\r\n.atencao {\r\n  text-align: center;\r\n  color: #ff1d23;\r\n  margin-bottom: 20px;\r\n}\r\n</style>"],"sourceRoot":"src/views/metas"}]}