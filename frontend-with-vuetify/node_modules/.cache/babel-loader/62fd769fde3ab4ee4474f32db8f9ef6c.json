,KAAA,CAAA,SAAA;AACA,KAFA;AAGA,GArDA;AAsDA,EAAA,OAAA,EAAA;AACA,IAAA,SADA,uBACA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,GADA,oBACA,MAAA,CAAA,IAAA,CAAA,EADA;AAAA;AAAA,uBAEA,GAAA,CACA,GADA,CACA,GADA,EAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA;AACA,kBAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AACA,iBALA,EAMA,KANA,CAMA,UAAA,IAAA,EAAA,CAAA,CANA,CAFA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KAVA;AAWA,IAAA,MAXA,oBAWA;AAAA;;AACA,MAAA,GAAA,CACA,GADA,kBACA,KAAA,IAAA,CAAA,EADA,GACA,KAAA,QADA,EAEA,IAFA,CAEA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,MAAA,CAAA,cAAA;AACA,OAJA,EAKA,KALA,CAKA,SALA;AAMA;AAlBA;AAtDA,CAAA","sourcesContent":["<template>\r\n  <v-container v-if=\"loading\">\r\n    <v-progress-circular indeterminate color=\"primary\"></v-progress-circular>\r\n  </v-container>\r\n\r\n  <v-container v-else>\r\n    <div class=\"perfil-main elevation-15\">\r\n      <div class=\"perfil-header\">\r\n        <div class=\"gravatar-img\">\r\n          <Gravatar :email=\"user.email\" alt=\"User\" />\r\n          <v-badge\r\n            v-if=\"user.admin\"\r\n            color=\"blue\"\r\n            icon=\"mdi-check-bold\"\r\n            bottom\r\n            overlap\r\n          ></v-badge>\r\n        </div>\r\n        <h1>{{ userData.name }}</h1>\r\n      </div>\r\n      <div class=\"perfil-content\">\r\n        <v-row>\r\n          <v-col cols=\"12\" md=\"3\" sm=\"12\">\r\n            <v-text-field\r\n              label=\"Nome\"\r\n              v-model=\"userData.name\"\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n          <v-col cols=\"12\" md=\"3\" sm=\"12\">\r\n            <v-text-field\r\n              label=\"E-mail\"\r\n              v-model=\"userData.email\"\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n          <v-col cols=\"12\" md=\"3\" sm=\"12\">\r\n            <v-text-field\r\n              v-if=\"userData.ong\"\r\n              v-model=\"userData.cnpj\"\r\n              label=\"CNPJ\"\r\n              v-mask=\"'##.###.###/####-##'\"\r\n              outlined\r\n            ></v-text-field>\r\n            <v-text-field\r\n              v-else\r\n              label=\"CPF\"\r\n              v-model=\"userData.cpf\"\r\n              v-mask=\"'###.###.###-##'\"\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n          <v-col cols=\"12\" md=\"3\" sm=\"12\">\r\n            <v-text-field\r\n              label=\"Telefone\"\r\n              v-model=\"userData.telefone\"\r\n              v-mask=\"'(##) #####-####'\"\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n        </v-row>\r\n\r\n        <v-row>\r\n          <v-col cols=\"12\" md=\"6\" sm=\"12\">\r\n            <v-text-field\r\n              label=\"Senha\"\r\n              v-model=\"userData.password\"\r\n              type=\"password\"\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n          <v-col cols=\"12\" md=\"6\" sm=\"12\">\r\n            <v-text-field\r\n              label=\"Confime sua Senha\"\r\n              v-model=\"userData.confirmPassword\"\r\n              type=\"password\"\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n        </v-row>\r\n\r\n        <v-divider class=\"divider-endereco\"></v-divider>\r\n\r\n        <v-row>\r\n          <v-col cols=\"12\" md=\"3\" sm=\"12\">\r\n            <v-text-field\r\n              label=\"Endereço\"\r\n              v-model=\"userData.endereco\"\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n          <v-col cols=\"12\" md=\"3\" sm=\"12\">\r\n            <v-text-field\r\n              label=\"Bairro\"\r\n              v-model=\"userData.bairro\"\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n          <v-col cols=\"12\" md=\"3\" sm=\"12\">\r\n            <v-text-field\r\n              label=\"Número\"\r\n              v-model=\"userData.numero\"\r\n              type=\"number\"\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n          <v-col cols=\"12\" md=\"3\" sm=\"12\">\r\n            <v-text-field\r\n              label=\"Complemento\"\r\n              v-model=\"userData.complemento\"\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n        </v-row>\r\n\r\n        <v-row>\r\n          <v-col cols=\"12\" md=\"4\" sm=\"12\">\r\n            <v-select\r\n              :items=\"estados\"\r\n              prepend-inner-icon=\"mdi-sign-real-estate\"\r\n              v-model=\"userData.estado\"\r\n              label=\"Estado\"\r\n              outlined\r\n            ></v-select>\r\n          </v-col>\r\n          <v-col cols=\"12\" md=\"4\" sm=\"12\">\r\n            <v-text-field\r\n              label=\"Cidade\"\r\n              v-model=\"userData.cidade\"\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n          <v-col cols=\"12\" md=\"4\" sm=\"12\">\r\n            <v-text-field\r\n              label=\"CEP\"\r\n              v-model=\"userData.cep\"\r\n              v-mask=\"'#####-###'\"\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n        </v-row>\r\n        <v-btn color=\"success\" @click=\"update\">\r\n          <v-icon left> mdi-account-edit </v-icon>\r\n          Atualizar\r\n        </v-btn>\r\n\r\n        <v-dialog v-model=\"dialog\" persistent max-width=\"600px\">\r\n          <template v-slot:activator=\"{ on, attrs }\">\r\n            <v-btn\r\n              color=\"primary\"\r\n              class=\"btn-new-password\"\r\n              dark\r\n              v-bind=\"attrs\"\r\n              v-on=\"on\"\r\n            >\r\n              <v-icon left> mdi-form-textbox-password </v-icon>\r\n              Nova senha\r\n            </v-btn>\r\n          </template>\r\n          <v-card>\r\n            <v-card-title>\r\n              <span class=\"headline\">Alterar Senha</span>\r\n            </v-card-title>\r\n            <v-card-text>\r\n              <v-container>\r\n                <v-row>\r\n                  <v-col cols=\"12\">\r\n                   <v-text-field\r\n                      :append-icon=\"show1 ? 'mdi-eye' : 'mdi-eye-off'\"\r\n                      :rules=\"[rules.required]\"\r\n                      :type=\"show1 ? 'text' : 'password'\"\r\n                      name=\"input-10-1\"\r\n                      label=\"Senha Atual*\"\r\n                      counter\r\n                      @click:append=\"show1 = !show1\"\r\n                    ></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\">\r\n                    <v-text-field\r\n                      :append-icon=\"show2 ? 'mdi-eye' : 'mdi-eye-off'\"\r\n                      :rules=\"[rules.required]\"\r\n                      :type=\"show2 ? 'text' : 'password'\"\r\n                      name=\"input-10-1\"\r\n                      label=\"Nova Senha*\"\r\n                      counter\r\n                      @click:append=\"show2 = !show2\"\r\n                    ></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\">\r\n                    <v-text-field\r\n                      :append-icon=\"show3 ? 'mdi-eye' : 'mdi-eye-off'\"\r\n                      :rules=\"[rules.required]\"\r\n                      :type=\"show3 ? 'text' : 'password'\"\r\n                      name=\"input-10-1\"\r\n                      label=\"Confirmar Nova Senha*\"\r\n                      counter\r\n                      @click:append=\"show3 = !show3\"\r\n                    ></v-text-field>\r\n                  </v-col>\r\n                </v-row>\r\n              </v-container>\r\n              <small>* indica campo necessário</small>\r\n            </v-card-text>\r\n            <v-card-actions>\r\n              <v-spacer></v-spacer>\r\n              <v-btn color=\"red darken-1\" text @click=\"dialog = false\">\r\n                Cancelar\r\n              </v-btn>\r\n              <v-btn color=\"green darken-1\" text @click=\"dialog = false\">\r\n                Salvar\r\n              </v-btn>\r\n            </v-card-actions>\r\n          </v-card>\r\n        </v-dialog>\r\n      </div>\r\n    </div>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport { userKey } from \"@/global\";\r\nimport { mapState } from \"vuex\";\r\nimport Gravatar from \"vue-gravatar\";\r\nimport { showError } from \"@/global\";\r\nimport api from \"../../config/api\";\r\n\r\nexport default {\r\n  name: \"Perfil\",\r\n  components: { Gravatar },\r\n  computed: mapState([\"user\"]),\r\n\r\n  data: function () {\r\n    return {\r\n      userData: {},\r\n      loading: true,\r\n      dialog: false,\r\n      show1: false,\r\n      show2: true,\r\n      show3: false,\r\n      rules: {\r\n        required: (value) => !!value || \"Requerido.\",\r\n      },\r\n      estados: [\r\n        { text: \"Acre\", value: \"AC\" },\r\n        { text: \"Alagoas\", value: \"AL\" },\r\n        { text: \"Amapá\", value: \"AP\" },\r\n        { text: \"Amazonas\", value: \"AM\" },\r\n        { text: \"Bahia\", value: \"BA\" },\r\n        { text: \"Ceará\", value: \"CE\" },\r\n        { text: \"Distrito Federal\", value: \"DF\" },\r\n        { text: \"Espírito Santo\", value: \"ES\" },\r\n        { text: \"Goiás\", value: \"GO\" },\r\n        { text: \"Maranhão\", value: \"MA\" },\r\n        { text: \"Mato Grosso\", value: \"MT\" },\r\n        { text: \"Mato Grosso do Sul\", value: \"MS\" },\r\n        { text: \"Minas Gerais\", value: \"MG\" },\r\n        { text: \"Pará\", value: \"PA\" },\r\n        { text: \"Paraíba\", value: \"PB\" },\r\n        { text: \"Paraná\", value: \"PR\" },\r\n        { text: \"Pernambuco\", value: \"PE\" },\r\n        { text: \"Piauí\", value: \"PI\" },\r\n        { text: \"Rio de Janeiro\", value: \"RJ\" },\r\n        { text: \"Rio Grande do Norte\", value: \"RN\" },\r\n        { text: \"Rio Grande do Sul\", value: \"RS\" },\r\n        { text: \"Rondônia\", value: \"RO\" },\r\n        { text: \"Roraima\", value: \"RR\" },\r\n        { text: \"Santa Catarina\", value: \"SC\" },\r\n        { text: \"São Paulo\", value: \"SP\" },\r\n        { text: \"Sergipe\", value: \"SE\" },\r\n        { text: \"Tocantins\", value: \"TO\" },\r\n      ],\r\n    };\r\n  },\r\n\r\n  mounted() {\r\n    this.loadUsers();\r\n    this.$root.$once(\"user-updated\", () => {\r\n      this.loadUsers();\r\n    });\r\n  },\r\n  methods: {\r\n    async loadUsers() {\r\n      const url = `/users/${this.user.id}`;\r\n      await api\r\n        .get(url)\r\n        .then((res) => {\r\n          this.userData = res.data;\r\n          this.loading = false;\r\n        })\r\n        .catch((erro) => {});\r\n    },\r\n    update() {\r\n      api\r\n        .put(`/users/${this.user.id}`, this.userData)\r\n        .then(() => {\r\n          this.$toasted.global.defaultSuccess();\r\n        })\r\n        .catch(showError);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n<style>\r\n.perfil-main {\r\n  background-color: #fff;\r\n  padding: 30px;\r\n  border-radius: 5px;\r\n}\r\n\r\n.perfil-header {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-content: center;\r\n  align-items: center;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.gravatar-img img {\r\n  border-radius: 50%;\r\n}\r\n\r\n.divider-endereco {\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.btn-new-password {\r\n  margin-left: 20px;\r\n}\r\n</style>"],"sourceRoot":"src/views/perfil"}]}