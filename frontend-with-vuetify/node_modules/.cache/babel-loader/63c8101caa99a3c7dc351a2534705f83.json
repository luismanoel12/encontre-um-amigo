ById\"\r\n\r\n\r\nimport { userKey } from \"@/global\"\r\n\r\nVue.use(VueRouter)\r\n\r\nconst routes = [{\r\n    name: 'home',\r\n    path: '/',\r\n    component: Home\r\n},\r\n{\r\n    name: 'auth',\r\n    path: '/auth',\r\n    component: Auth,\r\n    meta: { requiresOff: true }\r\n},\r\n{\r\n    name: 'carousel',\r\n    path: '/carousel',\r\n    component: Carousel,\r\n    meta: { requiresAdmin: true }\r\n},\r\n{\r\n    name: 'perfil',\r\n    path: '/perfil',\r\n    component: Perfil,\r\n    meta: { requiresOff: false }\r\n},\r\n{\r\n    name: 'metas',\r\n    path: '/metas',\r\n    component: Metas,\r\n    meta: { requiresOff: false }\r\n},\r\n{\r\n    name: 'metasList',\r\n    path: '/metas-list',\r\n    component: MetasList,\r\n    meta: { requiresOff: false }\r\n},\r\n{\r\n    name: 'metasById',\r\n    path: '/metas/:id',\r\n    component: MetasById,\r\n    meta: { requiresOff: false }\r\n},\r\n{\r\n    name: 'error404',\r\n    path: '/*',\r\n    component: Error404\r\n}]\r\n\r\n\r\nconst router = new VueRouter({\r\n    // mode: 'history',\r\n    routes \r\n})\r\n\r\nrouter.beforeEach((to, from, next) => {\r\n    const json = localStorage.getItem(userKey)\r\n\r\n    if(to.matched.some(record => record.meta.requiresAdmin)){\r\n        const user = JSON.parse(json)\r\n        user && user.admin ? next() : next({ path: '/' })\r\n    } else {\r\n        next()\r\n    }\r\n\r\n    if(to.matched.some(record => record.meta.requiresOff)){\r\n        const user = JSON.parse(json)\r\n        !user ? next() : next({ path: '/' })\r\n    } else {\r\n        next()\r\n    }\r\n})\r\n\r\nexport default router\r\n"]}]}