     label=\"E-mail\"\r\n              v-model=\"user.email\"\r\n              readonly\r\n              required\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n        </v-row>\r\n\r\n        <v-row>\r\n          <v-col cols=\"12\" sm=\"6\">\r\n            <v-text-field\r\n              label=\"Administrador\"\r\n              v-model=\"user.admin\"\r\n              readonly\r\n              required\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n          <v-col cols=\"12\" sm=\"6\">\r\n            <v-text-field\r\n              label=\"ONG\"\r\n              v-model=\"user.ong\"\r\n              readonly\r\n              required\r\n              outlined\r\n            ></v-text-field>\r\n          </v-col>\r\n        </v-row>\r\n\r\n        <div class=\"buttons\">\r\n          <v-btn\r\n            depressed\r\n            v-if=\"user.admin\"\r\n            @click=\"setAdmin\"\r\n            color=\"error\"\r\n          >\r\n            Remover Admin\r\n            <v-icon dark right> mdi-account-remove </v-icon>\r\n          </v-btn>\r\n          <v-btn\r\n            depressed\r\n            v-else\r\n            @click=\"setAdmin\"\r\n            color=\"success\"\r\n          >\r\n            Adicionar Admin\r\n            <v-icon dark right> mdi-account-lock </v-icon>\r\n          </v-btn>\r\n          <v-divider vertical></v-divider>\r\n          <v-btn\r\n            depressed\r\n            v-if=\"mode === 'remove'\"\r\n            @click=\"remove\"\r\n            color=\"error\"\r\n          >\r\n            Excluir\r\n            <v-icon dark right> mdi-delete </v-icon>\r\n          </v-btn>\r\n          <v-divider vertical></v-divider>\r\n          <v-btn depressed @click=\"reset\" class=\"btn-cancel\" color=\"primary\">\r\n            Cancelar\r\n            <v-icon dark right> mdi-close-thick </v-icon>\r\n          </v-btn>\r\n        </div>\r\n      </v-form>\r\n\r\n      <v-data-table :items=\"users\" :headers=\"headers\" class=\"elevation-10\">\r\n        <template v-slot:[`item.actions`]=\"{ item }\">\r\n          <v-btn\r\n            class=\"bt-actions\"\r\n            color=\"green\"\r\n            fab\r\n            elevation=\"0\"\r\n            small\r\n            @click=\"loadUser(item)\"\r\n            dark\r\n          >\r\n            <v-icon>mdi-pencil</v-icon>\r\n          </v-btn>\r\n\r\n          <v-btn\r\n            class=\"bt-actions\"\r\n            color=\"error\"\r\n            fab\r\n            elevation=\"0\"\r\n            small\r\n            @click=\"loadUser(item, 'remove')\"\r\n            dark\r\n          >\r\n            <v-icon>mdi-delete</v-icon>\r\n          </v-btn>\r\n        </template>\r\n      </v-data-table>\r\n    </v-container>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { showError } from \"@/global\";\r\nimport api from \"../../config/api\";\r\n\r\nexport default {\r\n  name: \"Users\",\r\n  data: function () {\r\n    return {\r\n      mode: \"save\",\r\n      user: {},\r\n      users: [],\r\n      headers: [\r\n        {\r\n          align: \"start\",\r\n          sortable: false,\r\n        },\r\n        { text: \"C√≥digo\", value: \"id\" },\r\n        { text: \"Nome\", value: \"name\" },\r\n        { text: \"E-mail\", value: \"email\" },\r\n        { text: \"Estado\", value: \"estado\" },\r\n        { text: \"Admin\", value: \"admin\" },\r\n        { text: \"Deletado em\", value: \"deletedAt\" },\r\n        { text: \"A√ß√µes\", value: \"actions\" },\r\n      ],\r\n    };\r\n  },\r\n  methods: {\r\n    loadUsers() {\r\n      const url = `/users`;\r\n      api.get(url).then((res) => {\r\n        this.users = res.data;\r\n      });\r\n    },\r\n    reset() {\r\n      this.mode = \"save\";\r\n      this.user = {};\r\n      this.loadUsers();\r\n    },\r\n    setAdmin() {\r\n      const method = \"put\";\r\n      const id = this.user.id;\r\n      api[method](`/setAdmin/${id}`, this.user)\r\n        .then(() => {\r\n          this.$toasted.global.defaultSuccess();\r\n          this.reset();\r\n        })\r\n        .catch(showError);\r\n    },\r\n    remove() {\r\n      const id = this.user.id;\r\n      api\r\n        .delete(`/users/${id}`)\r\n        .then(() => {\r\n          this.$toasted.global.defaultSuccess();\r\n          this.reset();\r\n        })\r\n        .catch(showError);\r\n    },\r\n    loadUser(user, mode = \"save\") {\r\n      this.mode = mode;\r\n      api.get(`/users/${user.id}`).then((res) => (this.user = res.data));\r\n    },\r\n  },\r\n  watch: {\r\n    page() {\r\n      this.loadUsers();\r\n    },\r\n  },\r\n  mounted() {\r\n    this.loadUsers();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":"src/views/users"}]}